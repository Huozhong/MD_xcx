/**contact.wxss**/
@import "../common/common.wxss";
page{
	height: 100%;
}
.container{
	height: auto;
	min-height: 100%;
	background-color: #efefef;
}

.conentWrap{
	width: 100%;
	height: 100%;
	font-size: 30rpx;
	padding: 0 20rpx;
}

.photos{
	width: 80%;
	height: 100%;
	position: absolute;
	left: 50%;
	transform: translateX(-50%);
}

.photos image{
	display: block;
	width: 90%;
	height: 100%;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}
